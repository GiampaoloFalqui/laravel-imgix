{
    "name": "nasyrov/laravel-imgix",
    "type": "library",
    "description": "Laravel support for imgix.",
    "keywords": [
        "laravel",
        "imgix",
        "cdn"
    ],
    "homepage": "https://github.com/nasyrov/laravel-imgix",
    "license": "MIT",
    "authors": [
        {
            "name": "Evgenii Nasyrov",
            "email": "inasyrov@ya.ru",
            "homepage": "https://github.com/nasyrov",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "~5.4",
        "imgix/imgix-php": "~1.1"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.7",
        "squizlabs/php_codesniffer": "~3.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Nasyrov\\Laravel\\Imgix\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nasyrov\\Laravel\\Imgix\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
